<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.learningenglish">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"

        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LearningEnglish">
        <activity
            android:name=".activity_Register"
            android:exported="true" />
        <activity
            android:name=".activity_Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
          </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
        </activity>

        <activity android:name=".activity_EnglishQuiz" />
        <activity android:name=".activity_ResultEnglishQuiz" />
        <activity android:name=".activity_nhap" />

        <activity android:name=".ActivityCorrectWord"/>
        <activity android:name=".ActivityResultCorrectWord"/>
        <activity android:name=".ActivityInputCorrectWord"/>
        <activity android:name=".ActivityHighScore"/>
        <activity android:name=".ActivityHighScoreCorrectWord"/>
        <activity android:name=".ActivityHighScoreEnglishQuiz"/>
        <activity android:name=".ActivityTutorialCorrectWord"/>
        <activity android:name=".ActivityTutorialEnglishQuiz"/>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>